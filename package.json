{
  "name": "bonsai-sat-tutor",
  "version": "1.0.0",
  "description": "AI-powered SAT preparation with Glass-inspired Bonsai tutor",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "dev:all": "concurrently \"npm run dev\" \"supabase start\"",
    "dev:watch": "concurrently \"npm run dev\" \"npm run type-check -- --watch\"",
    "db:reset": "supabase db reset",
    "db:generate-types": "supabase gen types typescript --local > src/types/database.ts",
    "db:seed": "supabase db seed",
    "deploy": "vercel --prod",
    "health-check": "node scripts/health-check.js",
    "monitor": "node scripts/monitor.js",
    "analyze": "cross-env ANALYZE=true next build",
    "bundle-analyzer": "@next/bundle-analyzer",
    "check-ai-config": "node scripts/check-ai-config.js",
    "check-supabase": "node scripts/check-supabase.js",
    "check-stripe": "node scripts/check-stripe.js",
    "debug:bonsai": "node scripts/debug-bonsai.js",
    "debug:stripe": "node scripts/debug-stripe.js",
    "test:glass": "node scripts/test-glass-integration.js",
    "test:webhooks": "node scripts/test-webhooks.js",
    "test:performance": "lighthouse-ci autorun",
    "optimize-images": "next-optimized-images",
    "update-deps": "npm-check-updates -u",
    "performance-audit": "node scripts/performance-audit.js",
    "backup-check": "node scripts/backup-check.js",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "next": "^14.2.30",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.6.3",
    "@supabase/supabase-js": "^2.46.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "stripe": "^16.12.0",
    "@stripe/stripe-js": "^4.8.0",
    "@stripe/react-stripe-js": "^2.8.0",
    "openai": "^4.70.0",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.1.10",
    "@heroicons/react": "^2.1.5",
    "framer-motion": "^11.11.9",
    "tailwindcss": "^3.4.14",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4",
    "lucide-react": "^0.294.0",
    "react-hot-toast": "^2.5.2",
    "zustand": "^5.0.1",
    "react-query": "^3.39.3",
    "axios": "^1.7.7",
    "zod": "^3.23.8",
    "react-hook-form": "^7.53.2",
    "@hookform/resolvers": "^3.10.0",
    "date-fns": "^4.1.0",
    "recharts": "^2.12.7",
    "three": "^0.169.0",
    "@react-three/fiber": "^8.17.10",
    "@react-three/drei": "^9.114.3",
    "lottie-react": "^2.4.0",
    "react-speech-recognition": "^3.10.0",
    "speech-synthesis-react": "^1.0.0",
    "react-hotkeys-hook": "^4.5.1",
    "react-intersection-observer": "^9.13.1",
    "react-virtual": "^2.10.4",
    "use-debounce": "^10.0.4",
    "nanoid": "^5.0.8",
    "crypto-js": "^4.2.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "rate-limiter-flexible": "^5.0.3",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "sharp": "^0.34.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "vaul": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/crypto-js": "^4.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/three": "^0.169.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.30",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "@next/bundle-analyzer": "^14.2.30",
    "cross-env": "^7.0.3",
    "concurrently": "^9.0.1",
    "jest": "^29.7.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "@playwright/test": "^1.48.2",
    "lighthouse-ci": "^0.14.0",
    "npm-check-updates": "^17.1.3",
    "lint-staged": "^15.2.10",
    "husky": "^9.1.6",
    "next-optimized-images": "^2.6.2",
    "supabase": "^1.200.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "sat",
    "tutor",
    "ai",
    "education",
    "bonsai",
    "glass",
    "next.js",
    "supabase",
    "stripe"
  ],
  "author": "Bonsai Education Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bonsai-education/bonsai-sat-tutor.git"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}